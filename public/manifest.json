{
  "manifest_version": 2,

  "name": "Cognition Apple",
  "description": "A productivity app to help you get the most out of your time by focusing effectively.",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "Apple - 16x16.png",
    "48": "Apple - 48x48.png",
    "128": "Apple - 128x128.png"
  },
  "permissions": [
    "webRequest",
    "*://developer.mozilla.org/",
    "*://youtube.com/*",
    "*://tiktok.com/*"
  ],
  "background": {
    "scripts": [ "app/background.js" ]
  },
  "short_name": "React App"
}
